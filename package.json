{
  "name": "cpp-cmake-project-generator",
  "displayName": "cpp + cmake project generator",
  "description": "An extension used for creating cpp projects with cmake.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:cpp-cmake-project-generator.createProject"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cpp-cmake-project-generator.createProject",
        "title": "C++ Project Generator: Create Project"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.51.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  }
}
